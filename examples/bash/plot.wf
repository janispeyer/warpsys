{
	"inputs": {
		"ld": "catalog:warpsys.org/gnu/glibc:v2.35:ld-amd64",
		"bash": "catalog:warpsys.org/gnu/bash:v5.1.16:amd64"
	},
	"steps": {
		"build": {
			"protoformula": {
				"inputs": {
					"/lib64": "pipe::ld",
					"/pkg/bash": "pipe::bash",
				},
				"action": {
					"script": {
						"interpreter": "/pkg/bash/bin/bash",
						"contents": [
							"echo 'hello from bash!'",
						]
					}
				},
				"outputs": {}
			}
		}
	},
	"outputs": {}
}
